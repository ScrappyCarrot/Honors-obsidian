formulas:
  Untitled: file(thumbnail)
views:
  - type: table
    name: Table
    filters:
      and:
        - file.ext == "pdf"
        - file.folder == "MEDIA/PDF"
    order:
      - formula.Untitled
      - file.ext
      - file.name
    sort:
      - property: file.ctime
        direction: DESC
    cardSize: 280
    imageAspectRatio: 1.05
    imageFit: contain
