formulas:
  Image: image(thumbnail)
views:
  - type: cards
    name: Table
    filters:
      and:
        - file.folder == "MEDIA/PDF/Books/History of Urbanism and Architecture Book 1/assets"
        - file.ext != "base"
    order:
      - file.name
      - formula.Image
      - file.ext
    image: file.file
    imageAspectRatio: 0.7
    cardSize: 200
