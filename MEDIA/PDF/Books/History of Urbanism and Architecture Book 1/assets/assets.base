formulas:
  Untitled: image(thumbnail)
properties:
  formula.Untitled:
    displayName: Thumbnail
views:
  - type: cards
    name: Table
    filters:
      and:
        - file.ext.contains("jpeg")
    order:
      - file.name
      - formula.Untitled
      - file.ext
    cardSize: 260
    imageAspectRatio: 1.25
    image: formula.Untitled
