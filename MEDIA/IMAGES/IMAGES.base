formulas:
  Image: image(thumbnail)
views:
  - type: cards
    name: Table
    filters:
      and:
        - file.folder == "MEDIA/IMAGES"
        - file.ext != "base"
    order:
      - file.name
      - formula.Image
      - file.ext
    sort:
      - property: file.mtime
        direction: DESC
    image: file.file
    imageAspectRatio: 0.7
    cardSize: 200
